{
  "expo": {
    "name": "Nexeed",
    "slug": "nexeed-social-app",
    "scheme": ["boltnexeed", "nexeed"],
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a",
      "dark": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mycompany.nexeed1",
      "userInterfaceStyle": "automatic",
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "dark": {
          "image": "./assets/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "config": {},
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "background-fetch", "background-processing"]
      }
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#00d4ff",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new notifications"
    },
    "android": {
      "icon": "./assets/icon.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "boltnexeed"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "nex-app-production.up.railway.app",
              "pathPrefix": "/post"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "nex-app-production.up.railway.app",
              "pathPrefix": "/profile"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.mycompany.nexeed1",
      "googleServicesFile": "./google-services.json",
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "dark": {
          "image": "./assets/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "softwareKeyboardLayoutMode": "resize",
      "userInterfaceStyle": "automatic",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.USE_FULL_SCREEN_INTENT"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.mycompany.nexeed1",
      "versionCode": 18
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96af36dc-9af8-4c34-86a7-c32539f13290"
      },
      "googleWebClientId": "775891172931-b6r8as807ahbuiclek3lvqmv2aqqolsa.apps.googleusercontent.com",
      "googleAndroidClientId": "775891172931-ncpt427eouiuf45eb918nfgdh73njcjn.apps.googleusercontent.com",
      "googleIosClientId": "775891172931-b6r8as807ahbuiclek3lvqmv2aqqolsa.apps.googleusercontent.com",
      "oneSignalAppId": "3be4f14c-d8b5-4d0b-8ddf-f9e73f813215"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ]
    ]
  }
}
